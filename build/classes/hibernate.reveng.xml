<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="banco" match-name="administrador" />
	<table-filter match-catalog="banco" match-name="cliente" />
	<table-filter match-catalog="banco" match-name="credito" />
	<table-filter match-catalog="banco" match-name="ejecutivo" />
	<table-filter match-catalog="banco" match-name="persona" />
	<table-filter match-catalog="banco" match-name="sucursal" />
	<table-filter match-catalog="banco" match-name="supervisor" />
	<table-filter match-name=".*"></table-filter>
</hibernate-reverse-engineering>